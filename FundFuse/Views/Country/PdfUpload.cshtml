@model  IEnumerable<TMP.Models.CountryMaster>
@{
    ViewBag.Title = "PdfUpload";
    Layout = "~/Views/MasterPage/_defaultLayout.cshtml";
}

@if (ViewBag.ErrorMessage != null)
{
    <label style="color:red;">@ViewBag.ErrorMessage</label>
}


<div class="col-md-12 div-border padoff">

    <div class="col-md-12">
        <a class="btn-tgs" href="~/Country/CreatPdfUpload">Create New</a>
    </div>

    <table id="example" class="display" cellspacing="0" width="100%">
        <thead>
            <tr class="bgcr">
                <th style="text-align:left !important">Company Type</th>
                <th style="text-align:left !important">FileName</th>
                <th style="text-align:left !important">Edit/Delete</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td style="text-align:left !important">@item.CustomerTypeName</td>
                    <td style="text-align:left !important">@item.Name</td>
                    <td style="text-align:left !important"><a href="../Country/CreatPdfUpload?PdfID=@item.PdfID">Edit</a> /<a href="../Country/PdfUpload?PdfID=@item.PdfID">Delete</a> </td>

                </tr>
            }

        </tbody>
    </table>
</div>

<div class="col-md-10" style="height:10px;"></div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript">
        //Mekar
        $(document).ready(function () {
            $('#example').DataTable({
                "pageLength": 10
            });
        });

    </script>
}