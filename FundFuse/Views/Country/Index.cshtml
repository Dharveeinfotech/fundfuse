<script type="text/javascript">
    $("#DynamicTextID").on("change", function () {
        var DynamicTextName = $("#DynamicTextID option:selected").text();
        var DynamicTextID = $("#DynamicTextID option:selected").val();
        $("#DynamicTextName").val(DynamicTextName);

        if (TemplateID == 0) {
            $("#DynamicTextName").val("");
        }
    });

    $("#DynamicTextTemplateFormSubmit").on("click", function () {
        $('#DynamicTextID').removeClass('validate[required]');
        $("#DynamicTextTemplateForm").validationEngine('attach', {
            promptPosition: "bottomLeft",
            scroll: false,
            autoHidePrompt: true,
            autoHideDelay: 3500,
            fadeDuration: 0.3,
            focusFirstField: false,
