    <script language="Javascript">
        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            }
            return true;
        }
    </script>
    <script language="Javascript">
        function Key(evt) {
            var k = evt.keyCode;
            return ((k > 64 && k < 91) || (k > 96 && k < 123) || k == 8 || (k >= 48 && k <= 57));
            $("#errorMsg").html("Sorry, Space is not allowed.");
            $('#Error').modal('show');
            return true;
        }
    </script>
    <script language="Javascript">
        function nospace(evt) {
            if (evt.value.match(/\s/g)) {
                $("#errorMsg").html("Sorry You Are Not Allowed to Enter Spaces.");
                $('#Error').modal('show');
                evt.value = evt.value.repalce(/\s/g);
                return true;
            }
            else { return false; }
        }
    </script>
