@model  IEnumerable<TMP.Models.UserMaster_ListAll_Result>
@if (Model.Count() > 0)
{
    <div class="tab-pane" id="4a">
        <!--tab 4a-->
        <div class="col-md-12" style="height:10px;"></div>
        <div class="col-md-12">
            <table id="example1" class="display" cellspacing="0" width="100%">
                <thead>
                    <tr class="bgspr">
                        <th>First Name</th>
                        <th>Middle Name</th>
                        <th>Last Name</th>
                        <th>Mobile Number</th>
                        <th>E-Mail ID</th>
                        <th>Edit</th>
                        <th>Delete</th>
                    </tr>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.FirstName</td>
                            <td>@item.MiddleName</td>
                            <td>@item.LastName</td>
                            <td>@item.MobileNo</td>
                            <td>@item.EmailID</td>
                            <td>@Html.ActionLink("Edit", "CreateUser", new { CustomerID = item.CustomerID, CustomerTypeID = item.CustomerTypeID, EditID = item.UserID, DeleteID = 0 })</td>
                            <td>@Html.ActionLink("Delete", "CreateUser", new { CustomerID = item.CustomerID, CustomerTypeID = item.CustomerTypeID, EditID = 0, DeleteID = item.UserID })</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>

    </div>
}
<script>
    $(document).ready(function () {
        $('#example1').DataTable({
            "pageLength": 5,

        });
    });
</script>

<style>
    .dataTables_filter {
        display: none;
    }
</style>




