                                                            }
                                                        }
                                                    </td>}

                                                <td>
                                                    @if (item.ChildCnt > 0)
                                                    {
                                                        <a href="javascript:void(0);" class="OpenList" data-id="@item.CustomerShareHoldID">View</a>
                                                    }
