@model  IEnumerable<TMP.Models.ThomsonReutersModel>
@if (Model.Count() > 0)
{
    <div class="tab-pane" id="4a">
        <!--tab 4a-->
        <div class="col-md-12" style="height:10px;"></div>
        <div class="col-md-12">
            <table id="example1" class="display" cellspacing="0" width="100%">
                <thead>
                    <tr class="bgspr">
                        <th>Organisation Name</th>
                        <th>Country Name</th>
                        <th>Case ID</th>
                        <th>Action</th>
                    </tr>
                <tbody>
                    @foreach (var item in Model)
                {
                        <tr>
                            <td>@item.CompanyName</td>
                            <td>@item.CountryName</td>
                            <td>@item.caseId</td>
                            @if (item.caseSystemId != "")
                            {
                                <td>@Html.ActionLink("View", "ScreeningResolution", new { IsAuthoData = true, AuthcaseSystemId = item.caseSystemId, TabIndex = 1 })</td>
                            }
                            else
                            {
                                <td></td>
                            }
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
}
<script>
    $(document).ready(function () {
        $('#example1').DataTable({
            "pageLength": 10,
        });
    });
</script>

