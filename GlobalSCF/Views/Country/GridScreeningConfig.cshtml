@model  IEnumerable<TMP.Models.ScreeningConfig>
@if (Model.Count() > 0)
{
    <div class="tab-pane" id="4a">
        <!--tab 4a-->
        <div class="col-md-12" style="height:10px;"></div>
        <div class="col-md-12">
            <table id="example" class="display" cellspacing="0" width="100%">
                <thead>
                    <tr class="bgspr">
                        <th>Category Name</th>
                        <th>Edit</th>
                        <th>Delete</th>
                    </tr>
                <tbody>
                    @foreach (var item in Model)
                {
                        <tr>
                            <td>@item.CategoryName</td>
                            <td>@Html.ActionLink("Edit", "ScreeningConfig", new { CategoryID = item.CategoryID })</td>
                            <td>@Html.ActionLink("Delete", "ScreeningConfig", new { dID = item.CategoryID })</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>

    </div>
}
<script>
    $(document).ready(function () {
        $('#example').DataTable({
            "pageLength": 5,
        });
     });
</script>

<style>
    .dataTables_filter {
        display: none;
    }
</style>

