@{
    ViewBag.Title = "Thank You";
    Layout = "~/Views/MasterPage/_TMPWebsite.cshtml";
}
<div class="col-md-12 container" style="margin-top:6%;">
    <form method="post" id="window">
        <div class="col-md-3"></div>
        <div class="col-md-6 div-thankmsg" style="padding-top:2%">
            @Html.Raw(ViewBag.ThankYouMsg)
            <div class="col-md-12" style="margin-top:5%; text-align:center;">
                <button id="Close" name="Close" value="Close" type="submit" class="btn btn-tgs">Close</button>
            </div>
        </div>
        <div class="col-md-3"></div>
    </form>

</div>
<style>
    .footeronline {
        position: fixed;
    }
</style>
<script>
    $("#Close").click(function () {
        document.location = '@Url.Action("Login", "mUserMasters")';
        return false;
    });
</script>