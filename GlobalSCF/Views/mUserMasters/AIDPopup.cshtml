<div class="popupbox col-md-10">

    @{
        var a = ViewData["ActionName"];
    }
    <div>
        <p class="col-md-11 popuptitle">Are you sure you want to @a</p>	<button class="closebtn col-md-1" onclick="HideDialog();" aria-label="close">&times;</button>
    </div>
    <div class="col-md-12" style="height:10px;"></div>
    <div class="col-md-12 form-group">
        <div class="col-md-3">Username</div>
        <div class="col-md-9"><input class="form-control" value="@ViewData["UserName"]" name="" type="text" disabled /></div>
        <label class="col-md-3">Remarks</label>
        <div class="col-md-9">
            <input type="text" id="txtProcessRemarks" class="form-control" style="height:60px;" name="txtProcessRemarks" required="" />
        </div>
        <div class="col-md-12" style="height:10px;"></div>
        <div style="text-align:center;">
            @if (a.Equals("Delete"))
            {
                <button id="btnDelete" name="btnDelete" class="btns" onclick="Act_InA_Del()">Delete</button>
            }
            else if (a.Equals("InActive"))
            {
                <button id="btnInActive" name="btnInActive" class="btns" onclick="Act_InA_Del()">InActive</button>
            }
            else if (a.Equals("Active"))
            {
                <button id="btnActive" name="btnActive" class="btns" onclick="Act_InA_Del()">Active</button>
            }

            <button id="btnClose" name="btnClose" onclick="HideDialog();" class="btns">Close</button>


        </div>
        <div class="col-md-12" style="height:10px;"></div>
    </div>

</div>

